{
  "version": "2.15.0",
  "name": "@lskjs/eslint-config",
  "description": "Eslint preset for LSK.js projects",
  "author": "Igor Suvorov <hi@isuvorov.com>",
  "license": "MIT",
  "repository": "https://github.com/lskjs/eslint-config",
  "keywords": [
    "eslint",
    "config",
    "lskjs"
  ],
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "release": "npm version minor -m \"chore(release): v%s\" && npm publish",
    "lint:js": "eslint . --ext .js  && prettier --write --plugin-search-dir=.",
    "lint:json": "eslint . --ext .json  && prettier --write --plugin-search-dir=.",
    "lint:js:fix": "eslint . --ext .js  --fix && prettier --write --plugin-search-dir=."
  },
    "dependencies": {
      "@typescript-eslint/eslint-plugin": "5.48.1",
      "@typescript-eslint/parser": "5.48.1",
      "eslint": "8.31.0",
            "eslint-config-airbnb": "19.0.4",
            "eslint-config-airbnb-base": "15.0.0",
            "eslint-config-prettier": "8.6.0",
            "eslint-import-resolver-typescript": "3.5.3",
            "eslint-plugin-import": "2.27.4",
            "eslint-plugin-json": "^3.1.0",
            "eslint-plugin-jsx-a11y": "6.7.1",
            "eslint-plugin-prettier": "4.2.1",
            "eslint-plugin-react": "7.32.0",
          "eslint-plugin-react-hooks": "4.6.0",
          "eslint-plugin-simple-import-sort": "8.0.0",
    "prettier": "2.8.2"
  },
  "optionalDependencies": {
    "typescript": "4.*.*"
  },
  "eslintConfig": {
    "extends": "./index.js"
  }
}
